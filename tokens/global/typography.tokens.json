{
  "$description": "Typography primitive tokens â€” from Figma Desktop (Inter)",
  "font": {
    "family": {
      "$type": "fontFamily",
      "titles":    { "$value": "Inter" },
      "content":   { "$value": "Inter" },
      "secondary": { "$value": "Open Sauce One" }
    },
    "weight": {
      "$type": "fontWeight",
      "regular":  { "$value": 400 },
      "medium":   { "$value": 500 },
      "semibold": { "$value": 600 },
      "bold":     { "$value": 700 }
    },
    "size": {
      "$type": "dimension",
      "72": { "$value": { "value": 72, "unit": "px" } },
      "64": { "$value": { "value": 64, "unit": "px" } },
      "56": { "$value": { "value": 56, "unit": "px" } },
      "52": { "$value": { "value": 52, "unit": "px" } },
      "48": { "$value": { "value": 48, "unit": "px" } },
      "44": { "$value": { "value": 44, "unit": "px" } },
