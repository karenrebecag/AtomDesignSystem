      "m":    { "$value": { "value": 32, "unit": "px" } },
      "md":   { "$value": { "value": 32, "unit": "px" } },
      "sm":   { "$value": { "value": 24, "unit": "px" } },
      "xs":   { "$value": { "value": 16, "unit": "px" } },
      "xxs":  { "$value": { "value": 16, "unit": "px" } }
    },
    "letterSpacing": {
      "$type": "dimension",
      "tight":  { "$value": { "value": -4, "unit": "px" } },
      "snug":   { "$value": { "value": -2, "unit": "px" } },
      "normal": { "$value": { "value": 0, "unit": "px" } }
    }
